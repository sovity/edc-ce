{
  "name": "edc-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "cold-start": "yarn && run-s env:local-dev api:generate next:dev",
    "cold-start-demo": "yarn && run-s env:local-dev api:generate next:build next:start",
    "start": "run-s env:local-dev next:dev",
    "start-demo": "run-s env:local-dev next:build next:start",
    "build-prod": "run-s next:test next:build",
    "dev": "yarn start",
    "next:lint": "next lint",
    "next:build": "cross-env SKIP_ENV_VALIDATION=true next build",
    "next:dev": "next dev",
    "next:start": "cp -r public .next/standalone && cp -r .next/static .next/standalone/.next && node .next/standalone/server.js",
    "next:test": "jest",
    "api:generate": "cd ../connector && node ../frontend/cross-platform-gradlew.cjs :ce:libs:api-clients:typescript-client:clean :ce:libs:api-clients:typescript-client:copyUiLib",
    "format-all": "prettier --write .",
    "use-client": "node ./force-use-client.cjs ./src",
    "translation:clean": "node ./remove-unused-translation-keys.cjs",
    "env:local-dev": "cp .env.local-dev .env"
  },
  "dependencies": {
